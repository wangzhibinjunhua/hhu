<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzb.hhu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:name=".interf.WApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity 
            android:name=".activity.LoginNewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <activity
            android:name=".activity.MainActivity"
            />
          <activity
            android:name=".activity.ReadDataActivity"
            />
          
          <activity
              android:name=".activity.EventLogActivity"/>
          
          <activity
              android:name=".activity.SettingTimeActivity"/>
        <activity
            android:name=".activity.AmmeterListActivity"
            android:windowSoftInputMode="stateHidden|stateAlwaysHidden"
            />
    </application>

</manifest>
